{
  "name": "sveltekit-s3-multipart-upload",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "description": "SvelteKit S3 Multipart Upload: how you can upload large files, such as video to your S3 compatible storage provider using presigned URLs.",
  "scripts": {
    "dev": "svelte-kit dev --port 3000 --https",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview --port 3000 --https",
    "check": "svelte-check --fail-on-hints",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:css": "stylelint \"src/**/*.{css,svelte}\"",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.113.0",
    "@aws-sdk/s3-request-presigner": "^3.113.0",
    "@aws-sdk/signature-v4-crt": "^3.110.0",
    "@aws-sdk/util-create-request": "^3.110.0",
    "@aws-sdk/util-format-url": "^3.110.0",
    "@fontsource/libre-franklin": "^4.5.8",
    "@fontsource/rajdhani": "^4.5.8",
    "@sveltejs/kit": "next",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "formidable": "^2.0.1",
    "husky": "^8.0.1",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-s3-multipart-upload"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-s3-multipart-upload/issues"
  },
  "type": "module"
}
